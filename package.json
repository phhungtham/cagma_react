    "build": "env-cmd -f src/configs/envSetting/production.env set CI=false & craco build",
    "test": "craco test",
    "eject": "craco eject",
    "lint": "eslint --ext js,jsx src/",
    "lint:fix": "eslint --fix --ext js,jsx src/",
    "prettier": "prettier --check \"src/**/(**.jsx|*.js|**.scss|*.css)\"",
    "prettier:fix": "prettier --write \"src/**/(**.jsx|*.js|**.scss|*.css)\"",
    "storybook": "sb dev",
    "build-storybook": "sb build",
    "build:uat": "env-cmd -f src/configs/envSetting/uat.env craco build",
    "build:production": "env-cmd -f src/configs/envSetting/production.env craco build"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
